  "source": "pragma solidity ^0.5.0;\n\ncontract UserDB {\n\n    mapping(address => bool) public users;\n\n    function addUser(address _user) public {\n        users[_user] = true;\n    }\n\n    function deleteUser(address _user) public {\n        users[_user] = false;\n    }\n}",
